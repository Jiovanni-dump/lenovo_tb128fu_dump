<?xml version="1.0" encoding="utf-8"?>
<config>
        <!-- for some default allowed autoruan packages not in vendor/zui/apps -->
        <default_allow_auto_run package="com.google.android.gms" allowtype="hide" />
        <default_allow_auto_run package="com.android.phone" allowtype="hide" />
        <default_allow_auto_run package="com.android.inputmethod.latin" allowtype="hide" />
        <default_allow_auto_run package="com.google.android.webview" allowtype="hide" />

        <!-- Tecent app store, ZUIR-12616, ZUIR-12817 -->
        <default_allow_auto_run package="com.tencent.android.qqdownloader" allowtype="hide" />

        <!-- not preloaded app but need whitelist -->
        <hide-fg-notify package="com.tencent.mm" />
        <hide-alertwindow-notify package="com.tencent.mm"/>

        <!-- TABZUIR-2713 liujl32 refer to wangwq14 allow gms autorun in background to avoid gms crash -->
        <allow-bg-intents package="com.google.android.gms" />

        <!-- TABZUIS-4713 -->
        <default_allow_auto_run package="com.lenovo.udcplatform" allowtype="hide" />
        <default_allow_auto_run package="com.lenovo.hecplatform.hecagent" allowtype="hide" />
        <default_allow_auto_run package="com.lenovo.hec.lenovoextend" allowtype="hide" />

        <!-- ZUIS-21329 -->
        <default_allow_auto_run package="io.appium.uiautomator2.server" allowtype="hide" />
</config>
